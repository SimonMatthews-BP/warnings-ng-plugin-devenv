<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="UI Tests Warnings (Firefox)" type="JUnit" factoryName="JUnit">
    <module name="warnings-ng-ui-tests" />
    <useClassPathOnly />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="plugins.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <option name="PACKAGE_NAME" value="" />
    <option name="MAIN_CLASS_NAME" value="" />
    <option name="METHOD_NAME" value="" />
    <option name="TEST_OBJECT" value="pattern" />
    <option name="VM_PARAMETERS" value="-ea -Dwebdriver.gecko.driver=/opt/bin/geckodriver -DElasticTime.factor=2 -Dwebdriver.chrome.driver=/opt/bin/chromedriver" />
    <option name="PARAMETERS" value="" />
    <envs>
      <env name="BROWSER" value="firefox" />
    </envs>
    <patterns>
      <pattern testClass=".*[a-z]UiTest|SmokeTest" />
    </patterns>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>
